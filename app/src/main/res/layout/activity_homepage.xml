<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homepage">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="427dp"
        android:layout_height="563dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/homeland_water"
        tools:layout_editor_absoluteY="181dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="190dp"
        android:layout_height="86dp"
        android:fontFamily="@font/nunito"
        android:text="Welcome to your:"
        android:textAlignment="center"
        android:textColor="#005C4F"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumpen"
        android:text="Homeland"
        android:textColor="#151515"
        android:textSize="60sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="112dp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="325dp"
        android:layout_height="229dp"
        app:srcCompat="@drawable/homaland_games"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="192dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="290dp"
        android:layout_height="270dp"
        app:srcCompat="@drawable/homeland_progress"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="228dp"
        android:layout_height="339dp"
        app:srcCompat="@drawable/homeland_social"
        tools:layout_editor_absoluteX="-8dp"
        tools:layout_editor_absoluteY="305dp" />

    <Button
        android:id="@+id/socials_button"
        android:layout_width="210dp"
        android:layout_height="144dp"
        android:background="#00FFFFFF"
        android:fontFamily="@font/nunito"
        android:text="Socials"
        android:textSize="35sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="-10dp"
        tools:layout_editor_absoluteY="394dp" />

    <Button
        android:id="@+id/progress_button"
        android:layout_width="250dp"
        android:layout_height="177dp"
        android:background="#00FFFFFF"
        android:fontFamily="@font/nunito"
        android:text="Progress"
        android:textSize="35sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="567dp" />

    <Button
        android:id="@+id/games_button"
        android:layout_width="185dp"
        android:layout_height="143dp"
        android:background="#00FFFFFF"
        android:fontFamily="@font/nunito"
        android:text="Games"
        android:textSize="35sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="209dp" />
</androidx.constraintlayout.widget.ConstraintLayout>