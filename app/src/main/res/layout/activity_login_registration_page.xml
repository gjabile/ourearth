<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A4DAA1"
    tools:context=".login_registration_page">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:layout_marginBottom="34dp"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/correct_white_rectangle" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:rotation="180"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/correct_white_rectangle"
        tools:layout_editor_absoluteY="260dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:rotation="180"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/correct_white_rectangle"
        tools:layout_editor_absoluteY="260dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/correct_white_rectangle" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="144dp"
        android:layout_height="68dp"
        android:fontFamily="@font/nanumpen"
        android:text="Sign In"
        android:textColor="#005C4F"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="75dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="176dp"
        android:layout_height="67dp"
        android:fontFamily="@font/nanumpen"
        android:text="Register"
        android:textColor="#005C4F"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="359dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress5"
        android:layout_width="299dp"
        android:layout_height="51dp"
        android:background="#70B16D"
        android:ems="10"
        android:fontFamily="@font/nunito"
        android:hint="UserName:"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="175dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress6"
        android:layout_width="299dp"
        android:layout_height="51dp"
        android:background="#70B16D"
        android:ems="10"
        android:fontFamily="@font/nunito"
        android:hint="Email:"
        android:inputType="textWebEmailAddress"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="437dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress8"
        android:layout_width="299dp"
        android:layout_height="51dp"
        android:background="#70B16D"
        android:ems="10"
        android:fontFamily="@font/nunito"
        android:hint="UserName:"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="513dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress7"
        android:layout_width="299dp"
        android:layout_height="51dp"
        android:background="#70B16D"
        android:ems="10"
        android:fontFamily="@font/nunito"
        android:hint="Password:"
        android:inputType="textVisiblePassword"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="259dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress9"
        android:layout_width="299dp"
        android:layout_height="51dp"
        android:background="#70B16D"
        android:ems="10"
        android:fontFamily="@font/nunito"
        android:hint="Password:"
        android:inputType="textVisiblePassword"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="585dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="110dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/save_bbutton"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="645dp" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="110dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/save_bbutton"
        tools:layout_editor_absoluteX="250dp"
        tools:layout_editor_absoluteY="323dp" />

    <Button
        android:id="@+id/login_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        tools:layout_editor_absoluteX="267dp"
        tools:layout_editor_absoluteY="323dp" />

    <Button
        android:id="@+id/register_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        tools:layout_editor_absoluteX="256dp"
        tools:layout_editor_absoluteY="644dp" />

</androidx.constraintlayout.widget.ConstraintLayout>